{
  "manifest_version": 3,
  "name": "Extension Switcher",
  "description": "List all extensions and allows easy activation and deactivation.",
  "version": "1.0",
  "author": "Jefferson Dantas",
  "icons": {
    "16": "src/icons/icon-16.png",
    "19": "src/icons/icon-48.png",
    "38": "src/icons/icon-64.png",
    "48": "src/icons/icon-128.png",
    "128": "src/icons/icon-256.png"
  },
  "action": {
    "default_icon": {
      "16": "src/icons/browser-icon-19.png",
      "19": "src/icons/browser-icon-19.png",
      "38": "src/icons/browser-icon-38.png",
      "48": "src/icons/browser-icon-48.png",
      "128": "src/icons/browser-icon-128.png"
    },
    "default_title": "Extensions",
    "default_popup": "src/popup.html"
  },
  "permissions": ["management", "storage"]
}
